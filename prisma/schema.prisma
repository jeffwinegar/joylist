// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
    provider     = "mysql"
    url          = env("DATABASE_URL")
    relationMode = "prisma"
}

generator client {
    provider = "prisma-client-js"
}

model Business {
    id     String @id @default(cuid())
    name   String
    url    String
    phone  String
    userId String

    @@index([userId])
}
